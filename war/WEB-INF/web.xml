<?xml version="1.0" encoding="utf-8" standalone="no"?><web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.5" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">

  <servlet>
    <servlet-name>SystemServiceServlet</servlet-name>
    <servlet-class>com.google.api.server.spi.SystemServiceServlet</servlet-class>
    <init-param>
      <param-name>services</param-name>
      <param-value>com.google.sample.mobileassistant.CheckInEndpoint,com.google.sample.mobileassistant.DeviceInfoEndpoint,com.google.sample.mobileassistant.OfferEndpoint,com.google.sample.mobileassistant.PlaceEndpoint,com.google.sample.mobileassistant.RecommendationEndpoint</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>SystemServiceServlet</servlet-name>
    <url-pattern>/_ah/spi/*</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>MaintenanceTasksServlet</servlet-name>
    <servlet-class>com.google.sample.mobileassistant.MaintenanceTasksServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>MaintenanceTasksServlet</servlet-name>
    <url-pattern>/admin/buildsearchindex</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>RecommendationServlet</servlet-name>
    <servlet-class>com.google.sample.mobileassistant.RecommendationServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>RecommendationServlet</servlet-name>
    <url-pattern>/tasks/recommendations</url-pattern>
  </servlet-mapping>

  <servlet>
    <display-name>Remote API Servlet</display-name>
    <servlet-name>RemoteApiServlet</servlet-name>
    <servlet-class>com.google.apphosting.utils.remoteapi.RemoteApiServlet</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>RemoteApiServlet</servlet-name>
    <url-pattern>/remote_api</url-pattern>
  </servlet-mapping>

  <security-constraint>
    <web-resource-collection>
      <url-pattern>/tasks2/*</url-pattern>
    </web-resource-collection>
    <auth-constraint>
      <role-name>admin</role-name>
    </auth-constraint>
  </security-constraint>


</web-app>